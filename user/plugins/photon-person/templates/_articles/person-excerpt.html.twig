{% extends '_articles/article.html.twig' %}
{% use "_articles/_block/article_main_excerpt.html.twig" %}
{% set excerpt = true %}
{% block article %}
      <!-- user/plugins/photon-{{ page.template }}/templates/_articles/{{ page.template }}-excerpt.html.twig -->
      <article class="{{ page.template }} excerpt" data-url="{{ page.url }}">
        {{ block('article_figure') }}
        {{ block('article_header') }}
        {{ block('article_main_excerpt') }}
        {#% include '_asides/list-featured.html.twig' -%#}
        {{ block('article_footer') }}
      </article>
{% endblock %}
